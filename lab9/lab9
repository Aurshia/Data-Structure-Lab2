#include <iostream>
using namespace std;
class Node{
public:   
    int year;
    string eventname;
    Node *left;
    Node *right;
    Node(int y  , string name){
        y = year;
        name = eventname;
        left = right = nullptr;
    }
};
Node *insert( Node *curr , int y , string name){
    if(curr == nullptr){
        return newnode(y , name);
    }
    if(curr->year == y){
        cout<<"Duplicate found";
        return y ;
    }
    else if(y < curr->year ){
        y->left = insert(curr->left , y , name); 
        return left;
    }
    else{
        y->right = insert(curr->right , y , name);
        return right;
    }
}
void inorder(Node *curr){
    if(curr == nullptr){
        return;
    }
    inorder(curr->left);
    cout<<curr->year;
    inorder(curr->right);
}
bool search(Node *curr , int y , string name){
    if(curr == nullptr){
        return false;
    }
    if(curr->year == y){
        return true;
    }
    else if(y < curr->year){
        return search(curr->left , y , name);
    }
    else{
        return search(curr->right , y , name);
    }
}
